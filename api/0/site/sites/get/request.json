
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Get sites of the user",
  "examples": ["GET https://api.xsitemanage.com/ext/0/site/sites?nextToken={nextToken}&maxPageSize={maxPageSize}"],
  "type": "object",
  "required": ["queryStringParameters"],
  "properties": {
    "queryStringParameters": {
      "description": "The HTTP Query String Parameters in the URL",
      "type": "object",
      "required": ["siteId"],
      "additionalProperties": false,
      "properties": {
        "maxPageSize": {
          "description": "For paging. A page will not contain more items that the given value",
          "type": "integer"
        },
        "nextToken": {
          "description": "For paging. Only used if given by an API response",
          "type": "string"
        }
      }
    }
  }
}
